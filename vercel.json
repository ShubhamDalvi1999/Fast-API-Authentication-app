{
  "version": 2,
  "buildCommand": "cd frontend && npm install && npm run build",
  "outputDirectory": "frontend/build",
  "builds": [
    {
      "src": "backend/app/api/main.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/check",
      "dest": "/api/check.js"
    },
    {
      "src": "/api-test",
      "dest": "/api/index.py"
    },
    {
      "src": "/health",
      "dest": "/backend/app/api/main.py"
    },
    {
      "src": "/api/db-test",
      "dest": "/backend/app/api/main.py"
    },
    {
      "src": "/api/debug",
      "dest": "/backend/app/api/main.py"
    },
    {
      "src": "/api/init-db",
      "dest": "/backend/app/api/main.py"
    },
    {
      "src": "/api/health-check",
      "dest": "/backend/app/api/main.py"
    },
    {
      "src": "/api(/?.*)",
      "dest": "/backend/app/api/main.py"
    },
    {
      "src": "/auth(/?.*)",
      "dest": "/backend/app/api/main.py"
    },
    {
      "src": "/docs",
      "dest": "/backend/app/api/main.py"
    },
    {
      "src": "/openapi.json",
      "dest": "/backend/app/api/main.py"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "PYTHONPATH": ".",
    "VERCEL_ENV": "production"
  }
} 
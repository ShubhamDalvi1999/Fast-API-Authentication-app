{
  "version": 2,
  "builds": [
    {
      "src": "backend/app/api/main.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/index.py",
      "use": "@vercel/node"
    },
    {
      "src": "frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/api-test",
      "dest": "/api/index.py"
    },
    {
      "src": "/health",
      "dest": "backend/app/api/main.py"
    },
    {
      "src": "/api/db-test",
      "dest": "backend/app/api/main.py"
    },
    {
      "src": "/api/debug",
      "dest": "backend/app/api/main.py"
    },
    {
      "src": "/api/health-check",
      "dest": "backend/app/api/main.py"
    },
    {
      "src": "/api(/?.*)",
      "dest": "backend/app/api/main.py"
    },
    {
      "src": "/auth(/?.*)",
      "dest": "backend/app/api/main.py"
    },
    {
      "src": "/docs",
      "dest": "backend/app/api/main.py"
    },
    {
      "src": "/openapi.json",
      "dest": "backend/app/api/main.py"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.+\\.[a-zA-Z0-9]{2,})$",
      "dest": "frontend/build/$1"
    },
    {
      "src": "/(.*)",
      "dest": "frontend/build/index.html"
    }
  ],
  "env": {
    "PYTHONPATH": ".",
    "VERCEL_ENV": "production"
  },
  "outputDirectory": "frontend/build"
} 